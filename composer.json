{
  "name": "ingenerator/image-processing",
  "description": "libvips image processing library",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Craig Gosman",
      "email": "craig@ingenerator.com"
    }
  ],
  "require": {
    "php": "~8.0.0 || ~8.1.0",
    "ext-ffi": "*",
    "ingenerator/php-utils": "^1.17",
    "jcupitt/vips": "^2.1",
    "symfony/process": "^4.2"
  },
  "require-dev": {
    "johnkary/phpunit-speedtrap": "^3.3",
    "phpunit/phpunit": "^9.5",
    "symfony/filesystem": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\ImageProcessing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\unit\\Ingenerator\\ImageProcessing\\": "test"
    }
  },
  "config": {
    "platform": {
      "ext-ffi": "8.1",
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://php-packages.ingenerator.com"
    }
  ]
}
