{
  "name": "ingenerator/image-processing",
  "description": "libvips image processing library",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Craig Gosman",
      "email": "craig@ingenerator.com"
    }
  ],
  "require": {
    "php": "~8.3.0",
    "ext-ffi": "*",
    "ingenerator/php-utils": "^1.17 || ^2.0",
    "jcupitt/vips": "^2.1",
    "symfony/process": "^7.0"
  },
  "require-dev": {
    "ergebnis/phpunit-slow-test-detector": "^2.6",
    "phpunit/phpunit": "^11.0",
    "symfony/filesystem": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\ImageProcessing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "8.3",
      "ext-ffi": "8.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://php-packages.ingenerator.com"
    }
  ]
}
